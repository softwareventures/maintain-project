{
  "name": "@softwareventures/maintain-project",
  "version": "0.0.0-development",
  "description": "Automatically create and maintain TypeScript projects with standard settings for Software Ventures Limited",
  "keywords": [
    "automated",
    "create-project",
    "maintain-project",
    "template",
    "typescript"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/maintain-project",
  "bugs": "https://github.com/softwareventures/maintain-project/issues",
  "repository": "github:softwareventures/maintain-project",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint --fix . && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "maintain-project": "node . update",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "bin": {
    "maintain-project": "./index.js"
  },
  "sideEffects": [
    "./index.js"
  ],
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "dependencies": {
    "@eslint/eslintrc": "1.4.1",
    "@softwareventures/array": "6.1.0",
    "@softwareventures/chain": "3.0.0",
    "@softwareventures/date": "3.1.2",
    "@softwareventures/dictionary": "2.1.1",
    "@softwareventures/iterable": "3.3.0",
    "@softwareventures/nullable": "3.2.0",
    "@softwareventures/ordered": "2.0.0",
    "@softwareventures/promise": "2.0.0",
    "@softwareventures/template-node-project": "2.1.5",
    "@softwareventures/template-webpack-project": "2.3.25",
    "@types/empty-dir": "2.0.4",
    "@types/escodegen": "0.0.10",
    "@types/estree": "1.0.6",
    "@types/hosted-git-info": "3.0.5",
    "@types/jsdom": "21.1.7",
    "@types/node": "14.18.63",
    "@types/picomatch": "2.3.3",
    "@types/recursive-readdir": "2.2.4",
    "@types/semver": "7.5.8",
    "@types/spdx-correct": "3.1.3",
    "@types/wordwrap": "1.0.3",
    "commander": "10.0.1",
    "empty-dir": "3.0.0",
    "escodegen": "2.1.0",
    "hosted-git-info": "6.1.1",
    "jsdom": "21.1.2",
    "node-releases": "2.0.18",
    "picomatch": "2.3.1",
    "prettier-package-json": "2.8.0",
    "recursive-readdir": "2.2.3",
    "semver": "7.6.3",
    "simple-git": "3.27.0",
    "spdx-correct": "3.2.0",
    "spdx-license-list": "6.9.0",
    "tslib": "2.8.1",
    "tslint": "6.1.3",
    "typescript": "5.4.5",
    "unknown": "0.2.6",
    "wordwrap": "1.0.0",
    "xml-formatter": "2.6.1",
    "yaml": "1.10.2",
    "yarn": "1.22.22"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "7.9.0",
    "@softwareventures/prettier-config": "4.0.0",
    "@softwareventures/semantic-release-config": "3.1.0",
    "@softwareventures/tsconfig": "7.3.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "ava": "5.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "46.10.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-sonarjs": "0.23.0",
    "prettier": "2.8.8",
    "semantic-release": "19.0.5",
    "shx": "0.3.4",
    "ts-node": "10.9.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "publishConfig": {
    "access": "public"
  }
}
