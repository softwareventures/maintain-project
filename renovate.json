{
  "extends": ["github>softwareventures/renovate-config:app"],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchPackageNames": [
        "@softwareventures/template-node-project",
        "@softwareventures/template-webpack-project"
